<%--  Created by IntelliJ IDEA.  User: mee  Date: 2017/4/27  Time: 13:44  To change this template use File | Settings | File Templates.--%><%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ page import="java.util.List" %><%@ page import="entity.Table" %><%@ page import="entity.Booking" %><%    List<Table> availableTable = (List<Table>) request.getAttribute("availableTable");    Booking currentBooking = (Booking) request.getAttribute("currentBooking");%><html><head>    <title>调换桌号</title>    <link rel="stylesheet" type="text/css" href="./css/button.css">    <link rel="stylesheet" type="text/css" href="./css/center.css">    <link rel="stylesheet" type="text/css" href="css/title.css"></head><body><form method="post" action="/transfercomplete">    <p class="MyTitle">预定单</p>    <table class="setCenter">        <tr>            <td style="text-shadow: #f3f3f3 1px 1px 0px, #b2b2b2 1px 2px 0">原座位号</td>            <td class="MyTitle"><%= currentBooking.getTno() %></td>        </tr>        <tr>            <td style="text-shadow: #f3f3f3 1px 1px 0px, #b2b2b2 1px 2px 0">新座位号选择</td>            <td>                <select name="transferTableSelect">                    <%                        int freeTablesSize = availableTable.size();                        for (int i = 0; i < freeTablesSize; i++) {                            int tno = availableTable.get(i).getTno();                            int places = availableTable.get(i).getPlaces();                            if (i == 0) {                                if (tno != currentBooking.getTno()) {                                    out.print("<option  selected='selected' value ='"                                            + tno + "'>" + tno + "（" + places + "人左右）" + "</option>");                                }                            } else {                                if (tno != currentBooking.getTno()) {                                    out.print("<option  value ='"                                            + tno + "'>" + tno + "（" + places + "人左右）" + "</option>");                                }                            }                        }                    %>                </select>            </td>        </tr>    </table>    <%= "<input hidden name='currentBid' value='" + currentBooking.getBid() + "'>" %>    <input class="button2 green" type="submit" value="确定"></form></body></html>